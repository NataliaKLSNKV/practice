#include "pch.h"
#include <iostream>
#include <fstream>
#include <string>
using namespace std;

int main(int argc, const char * argv[]) {
    //Вариант 1
    fstream file("text.txt");
    string s;
    while (!file.eof()) {
        file >> s;
        cout << s << endl;
    }
    
    //Вариант 2 (Для прекращения ввода control + D или ctrl + z)
    fstream fileOut("fileOut.txt");
    while (cin >> s){
        fileOut << s << endl;
    }
    return 0;
}
